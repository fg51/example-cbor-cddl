schema = {
  format-key => format
  work-number-key => text  ; work number
  works-key => [* food-unit]
}

format-key = 0
work-number-key = 1
works-key = 2

format = {
  major-version => uint
  minor-version => uint
}

major-version = 0
minor-version = 1

food-unit = {
  serial-key => uint
  apple-key => weight
  banana-key => weight
}

serial-key = 0
apple-key = 1
banana-key = 2

weight = {
  before-key => float,
  after-key => float,
}

before-key = 0
after-key = 1
